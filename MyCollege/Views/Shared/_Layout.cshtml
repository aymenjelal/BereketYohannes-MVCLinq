<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @*@Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")*@

    @Styles.Render("~/css/mobirise-icons.css")  
    @Styles.Render("~/css/tether.min.css")
    @Styles.Render("~/css/bootstrap.min.css")
    @Styles.Render("~/css/bootstrap-grid.min.css")
    @Styles.Render("~/css/bootstrap-reboot.min.css")
    @Styles.Render("~/css/styles.css")
    @Styles.Render("~/css/style.css")
    @Styles.Render("~/css/mbr-additional.css")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("MyCollege", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">

                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/JavaScript/jquery.min.js")
    @Scripts.Render("~/JavaScript/popper.min.js")
    @Scripts.Render("~/JavaScript/tether.min.js")
    @Scripts.Render("~/JavaScript/bootstrap.min.js")
    @Scripts.Render("~/JavaScript/smooth-scroll.js")
    @Scripts.Render("~/JavaScript/jquery.mb.vimeo_player.js")
    @Scripts.Render("~/JavaScript/bootstrap-carousel-swipe.js")
    @Scripts.Render("~/JavaScript/jquery.mb.ytplayer.min.js")
    @Scripts.Render("~/JavaScript/jquery")
    @Scripts.Render("~/JavaScript/script.js")
    @Scripts.Render("~/JavaScript/sliderVideo/script.js")
    @RenderSection("scripts", required: false)
</body>
</html>
